{
  "manifest_version": 2,
  "name": "Zelda Anti-Phishing",
  "short_name": "Zelda",
  "description": "Zelda prevent phishing sites from being displayed",
  "version": "1.1.3",
  "author": "Segasec Labs LTD",
  "background": {
    "page": "html/background.html"
  },
  "browser_action": {
    "default_icon": "icons/icon_green.png",
    "default_popup": "html/optionPopup.html",
    "default_title": "Segasec"
  },
  "optional_permissions": [
    "history"
  ],
  "web_accessible_resources": [
    "css/alert.css",
    "html/warning.html",
    "icons/icon_green.png"
  ],
  "permissions": [
    "storage",
    "webRequest",
    "webRequestBlocking",
    "<all_urls>",
    "http://*/*",
    "https://*/*",
    "tabs",
    "activeTab",
    "https://www.google-analytics.com/*"
  ],
  "icons": {
    "128": "icons/segaSecIcon.png",
    "48": "icons/segaSecIcon48.png",
    "16": "icons/icon-segasec16.png"
  }
}
